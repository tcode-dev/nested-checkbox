<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Nested Checkbox</title>
    <link rel="stylesheet" href="<%- asset('/css/top/index.css') %>">
    <link href="data:image/svg+xml;base64," rel="icon" type="image/x-icon" />
</head>
<body>
<div class="container">
    <h1>Nested Checkbox</h1>
    <form action="/" id="j-form" class="form">
        <div class="form__group form__row--reverse">
            <input type="checkbox" id="j-indeterminate" class="form__checkbox">
            <label for="j-indeterminate" class="form__label">indeterminate</label>
        </div>
        <div class="form__group">
            <ul id="j-nestedCheckbox" class="list">
                <%- include('partial/list', { list }); %>
            </ul>
        </div>
        <div class="form__group form__row--between form__search">
            <p class="form__text"><span id="j-searchResult">0</span>ä»¶</p>
            <button type="submit" class="form__btn">Submit</button>
        </div>
    </form>
    <script src="<%- asset('/js/TopController.js') %>"></script>
    <script>
        (function() {
            var topController = new TopController();

            topController.init();
        }());
    </script>
</div>
</body>
</html>